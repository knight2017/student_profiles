
<%= simple_form_for [current_user, @profile, @project] do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :web_app_link %>
  <%= f.input :github_link%>
  <%= f.input :logo %>
  <%= f.association :project_tags, as: :check_boxes %>
  <%= f.button :submit, class: "btn-primary" %>
<% end %>
