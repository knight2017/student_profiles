<% experiences.each do |experience| %>
  <div>
  <h3>Job title: <%= experience.job_title %></h3>
  <h3>Company: <%= experience.company %></h3>
  <h3>Start at: <%= experience.start_date %></h3>
  <h3>End at: <%= experience.end_date %></h3>
  <h3>Current : <%=  experience.current ? "Yes" : "No" %></h3>
  <h3>Description: <%= experience.description %></h3>

    <a class='edit-link' data-id='<%= experience.id %>', data-user='<%= @user.id %>', data-profile='<%= @profile.id %>' data-type='experience'  href="#">Edit</a>
    <a class='delete-link' data-id='<%= experience.id %>', data-user='<%= @user.id %>', data-profile='<%= @profile.id %>' data-type="experience" href="#">Delete</a>
    <hr>
  </div>
<% end %>
