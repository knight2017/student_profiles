<!-- <section> -->
  <div class="show">
    <h4><p><%= image_tag education.logo.url(:thumb) %>&nbsp;<%= education.school %></p></h4>
      <div>
     <p>Start Date: <%= education.start_date %>&nbsp;to
     <% if education.current? %>
       Present
     <% else %>
       <%= education.end_date %>
     <% end %>
     </p>
     </div>
    <p>Summary: <%= education.description %></p>
    <p>Links: <a><%= education.link %></a> </p>

    <% if can? :manage, profile %>
      <%= link_to "Edit", user_profile_education_path(@user, profile, education) %>
      <%= link_to "Delete", user_profile_experience_path(@user, profile, education), method: :delete, data: {confirm: "Are you sure?"} %>
    <% end %>
    <%#= link_to "Add another school", new_education_path %>
  </div>
<!-- </section> -->
