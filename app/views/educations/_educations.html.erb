<% educations.each do |education| %>
  <h3>school: <%= education.school %></h3>
  <h3>description: <%= education.description %></h3>
  <h3>School Web: <%= education.link %></h3>
  <%= image_tag education.logo.url(:medium) %>

  <a class='edu-edit-link' data-id='<%= education.id %>', data-user='<%= user.id %>', data-profile='<%= profile.id %>' href="#">Edit</a>
  <a class='edu-del-link' data-id='<%= education.id %>', data-user='<%= user.id %>', data-profile='<%= profile.id %>' href="#">Delete</a>
   <hr>
<% end %>
